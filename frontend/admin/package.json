{
  "name": "admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:routes": "npx tsr generate",
    "generate:api:hosting": "npx swagger-typescript-api -p ./swagger.json -o ./src/types -n hostingApi.ts --extract-request-params --extract-request-body --extract-response-body --responses --unwrap-response-data",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:stop": "docker-compose -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.2.0",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-router": "^1.88.0",
    "monaco-editor": "^0.52.2",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swagger-typescript-api": "^13.0.23",
    "tailwind-merge": "^2.5.5",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@tanstack/router-devtools": "^1.88.0",
    "@tanstack/router-plugin": "^1.87.13",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
